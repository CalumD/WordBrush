    "@types/express": "latest",
    "@types/fs-extra": "latest",
    "@types/multer": "latest",
    "@types/node": "latest",
    "@types/serve-favicon": "latest",
    "@types/shelljs": "latest",
    "@types/uuid": "latest",
    "body-parser": "latest",
    "nodemon": "latest",
    "ts-node": "latest",
    "typescript": "latest"
  },
